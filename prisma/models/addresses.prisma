model Address {
    id         Int     @id @default(autoincrement())
    user       User    @relation(fields: [userId], references: [id])
    userId     Int
    country    String
    state      String
    city       String
    street     String
    postalCode String
    longitude  Float?
    latitude   Float?
    isDefault  Boolean @default(false)
    orders     Order[] @relation("AddressOrders")
    type       String?
}
